&general
  PREFIX = RbBr222
  MODE = optimize
  NAT = 64; NKD = 2
  KD = Rb Br
/

&optimize
  DFSET = DFSET_cubic
  FC2XML = RbBr222_harmonic.xml
/

&interaction
  NORDER = 2  # 1: harmonic, 2: cubic, ..
/

&cell
  1.88973 # convert Angstrom to Bohr
  14.044200 0.000000 0.000000
